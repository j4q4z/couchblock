{
    "manifest_version":2,
    "name": "Couch Block",
    "version":"0.0.1",
    "background":{
      "scripts":["JavaScript/background.js"],
      "persistance":false
    },
    "browser_action": {
        "default_popup": "HTML/popup.html",
        "default_icon":"images/icon.png"
      },
      "permissions": [
        "webRequestBlocking",
        "webRequest",
        "http://*/*",
        "https://*/*"
     ] 
}